{
  "name": "feedback-service",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "build": "npm run clean && npm run build-babel && npm run copy",
    "build-babel": "babel -d ./dist ./src -s",
    "clean": "rm -rf dist && mkdir dist",
    "copy": "babel src --out-dir ./dist --copy-files",
    "dev": "nodemon --exec babel-node src/app.js",
    "format": "prettier --write --cache \"**/*.{js,md,html}\"",
    "lint": "eslint src --ext js,md --ignore-path .gitignore --fix --cache --report-unused-disable-directives",
    "prepare": "husky",
    "start": "node dist/app.js",
    "test": "mocha --require @babel/register test/index.test.js --timeout 10000 --exit",
    "test:unit": "mocha --require @babel/register test/root.test.js && test/unit/**.test.js --timeout 10000 --exit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sliit-foss/functions": "^2.10.0",
    "@sliit-foss/http-logger": "^1.3.1",
    "babel-eslint": "10.1.0",
    "celebrate": "^15.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "http-errors": "2.0.0",
    "mongoose": "^8.3.0",
    "mongoose-aggregate-paginate-v2": "^1.0.7",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.24.4",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@sliit-foss/eslint-config-internal": "1.1.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/sinon": "^17.0.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-register": "^6.26.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "mocha": "^10.4.0",
    "chai": "4.3.10",
    "mongodb-memory-server": "^9.1.8",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sinon": "^17.0.1",
    "supertest": "6.3.4"
  }
}
